{
  "name": "redux-bundler",
  "version": "11.2.0",
  "author": "Henrik Joreteg <henrik@joreteg.com> (joreteg.com)",
  "dependencies": {
    "create-selector": "2.2.0",
    "feather-route-matcher": "3.0.0",
    "idb-keyval": "2.3.0",
    "querystringify": "0.0.4",
    "redux": "3.7.2",
    "reselect": "3.0.1",
    "ric-shim": "1.0.0",
    "url-parse": "1.1.3"
  },
  "devDependencies": {
    "buble": "0.15.2",
    "nodemon": "1.10.2",
    "npm-watch": "0.1.6",
    "rollup": "0.49.2",
    "rollup-plugin-json": "2.3.0",
    "standard": "8.3.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.0"
  },
  "files": [
    "index.js",
    "src"
  ],
  "jsnext:main": "src/index.js",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c > tmp.js && buble tmp.js > index.js && rm tmp.js",
    "prepublish": "standard && npm run build",
    "test": "npm run build && tape test/*.js && standard --fix",
    "watch": "npm-watch"
  },
  "standard": {
    "globals": [
      "window",
      "self"
    ]
  },
  "watch": {
    "test": "{src,test}/**.js"
  }
}

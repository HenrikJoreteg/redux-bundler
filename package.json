{
  "name": "redux-bundler",
  "version": "7.3.1",
  "author": "Henrik Joreteg <henrik@joreteg.com> (joreteg.com)",
  "dependencies": {
    "create-selector": "2.0.2",
    "feather-route-matcher": "2.0.1",
    "idb-keyval": "2.1.2",
    "querystringify": "0.0.4",
    "redux": "3.6.0",
    "ric-shim": "1.0.0",
    "url-parse": "1.1.3"
  },
  "devDependencies": {
    "buble": "^0.13.2",
    "nodemon": "1.10.2",
    "npm-watch": "0.1.6",
    "rollup": "0.34.13",
    "rollup-plugin-json": "2.0.1",
    "standard": "8.3.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.0"
  },
  "files": [
    "index.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c > tmp.js && buble tmp.js > index.js && rm tmp.js",
    "prepublish": "standard && npm run build",
    "test": "npm run build && tape test/*.js && standard --fix",
    "watch": "npm-watch"
  },
  "standard": {
    "globals": [
      "window",
      "self"
    ]
  },
  "watch": {
    "test": "{src,test}/**.js"
  }
}
